---
import type { Props  as ImgProps } from "./Img";
import { Img } from "./Img";

export interface Props extends ImgProps {
  alt: string;
  width?: number;
  height?: number;
}

const {className, ...props} = Astro.props


---
<Img client:idle className={`rounded-xs max-w-full ${className ?? ''}`} {...props} width={828} />